      "allowed":["url1","url2","url3"]
    },
    {
      "name": "processor_type",
      "type": "string"
    },
    {
      "name":"parameters",
      "type":"object"
    },
    {
      "name":"log",
      "type":"object"
    },
    {
      "name":"post_processes",
      "type":"array"
    }
  ],
  "output": [
    {
      "name": "responseCode",
      "type": "number"
    }
  ]
