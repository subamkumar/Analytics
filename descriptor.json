{
  "name": "Flogo-Analytics-activity",
  "type": "flogo:activity",
  "version": "0.0.1",
  "ref":"https://github.com/subamkumar/Flogo_Analytics",
  "title": "FLOGO Analytics",
  "description": "FLOGO Analytics",
  "homepage": "https://github.com/subamkumar/Flogo_Analytics",
  "settings":[
    {
      "name": "API Gateway URL",
      "type": "string",
      "required": true,
      "display":{
        "name":"API Gateway URL",
        "type":"dropdown",
        "selection":"single"
      },
      "allowed":["url1","url2","url3"]
    }
  ],
  "input": [
    {
      "name": "processType",
      "type": "string",
      "display":{
        "type":"dropdown",
        "selection":"single"
      },
      "allowed":["collections","locations","activities","processes"]
    },
    {
      "name": "getId",
      "type": "string"
    },
    {
      "name":"filterByCollectionId",
      "type":"string"
    },
    {
      "name":"filterByLocationId",
      "type":"string"
    },
    {
      "name":"filterByActivityId",
      "type":"string"
    }
  ],
  "output": [
    {
      "name": "responseCode",
      "type": "number"
    }
  ]
}
